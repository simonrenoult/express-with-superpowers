{
  "name": "express-with-superpowers",
  "version": "0.0.1",
  "description": "Opinionated configuration for Express API.",
  "main": "src/server.js",
  "author": "Simon Renoult <simon.renoult@gmail.com>",
  "license": "MIT",
  "keywords": [
    "express",
    "plugin",
    "plugins",
    "preset"
  ],
  "engines": {
    "node": ">= 12.10.0",
    "npm": ">= 6.10.3"
  },
  "scripts": {
    "coverage": "nyc npm test",
    "lint": "eslint .",
    "prestart": "npm run version:check",
    "pretest": "npm run version:check && npm run lint",
    "start": "node bin/start.js",
    "stop": "node bin/stop.js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_ENV=test mocha --recursive test/unit",
    "test:e2e": "NODE_ENV=test mocha --recursive test/e2e",
    "version:check": "check-node-version --package",
    "watch": "nodemon bin/start.js | bunyan"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "camelcase": "5.3.1",
    "check-node-version": "4.0.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "helmet": "3.21.0",
    "http-status": "1.3.2",
    "nconf": "0.10.0",
    "require-directory": "2.1.1",
    "response-time": "2.3.2",
    "serve-favicon": "2.5.0",
    "swagger-ui-express": "4.0.7"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.3.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "got": "9.6.0",
    "mocha": "6.2.0",
    "nodemon": "1.19.2",
    "nyc": "14.1.1",
    "prettier": "1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simonrenoult/express-with-superpowers"
  },
  "bugs": {
    "url": "https://github.com/simonrenoult/express-with-superpowers/issues"
  }
}
